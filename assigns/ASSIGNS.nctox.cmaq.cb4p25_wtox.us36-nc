      setenv REPINVEN $NEPINVEN 
      setenv ATSUP    $NTSUP    
      setenv ASSUP    $NSSUP    
      setenv AGSUP    $NGSUP    
      setenv ACREP    $NCREP    
      setenv APROJREP $NPROJREP 
