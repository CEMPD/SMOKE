#
## Conditional settings
   if ( $SMK_SOURCE == A && $NONROAD == Y ) then
      setenv ARINV   $NRINV
      setenv AREA    $NROAD  
      setenv ATMP    $NTMP   
      setenv ASMAT_S $NSMAT_S
      setenv ASMAT_L $NSMAT_L
      setenv ARMAT_S $NRMAT_S
      setenv ARMAT_L $NRMAT_L
      setenv ARSUP   $NRSUP  
