	 setenv REPMGSC_L   $REPSTAT/repmgs_l.$GRID.$ESCEN.rpt
	 setenv REPPGSC_L   $REPSTAT/reppgs_l.$GRID.$ESCEN.rpt
	 setenv REPEGSC_L   $REPSTAT/repegs_l.$GRID.$ESCEN.rpt
