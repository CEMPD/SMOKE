         setenv REPEGS_S    $REPSTAT/repegs_s.$GRID.$ESCEN.rpt

         setenv REPAGSC_S   $REPORTS/$ASCEN/static/repagsc_s.$SRCABBR.$GRID.$ASCEN.rpt
         setenv REPMGSC_S   $REPORTS/$MSCEN/static/repmgsc_s.$SRCABBR.$GRID.$MSCEN.rpt
