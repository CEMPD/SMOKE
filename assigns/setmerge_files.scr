	 setenv REPMGTC   $REPSCEN/repmgt.$ESDATE.$NDAYS.$GRID.$ESCEN.rpt
	 setenv REPPGTC   $REPSCEN/reppgt.$ESDATE.$NDAYS.$GRID.$ESCEN.rpt
