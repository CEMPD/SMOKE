         setenv REPMGT    $REPORTS/$MSCEN/scenario/repmgt.$SRCABBR.$ESDATE.$NDAYS.$GRID.$MSCEN.rpt
         setenv REPPGT    $REPORTS/$PSCEN/scenario/reppgt.$SRCABBR.$ESDATE.$NDAYS.$GRID.$PSCEN.rpt
