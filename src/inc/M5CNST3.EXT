C.........  Dimensioning parameters
        INTEGER    NM5VPOL          ! Maximum number of Mobile5 volatile pols
        INTEGER    MXM5EPR          ! Maximum number of emission processes
        INTEGER    MXM5NDI          ! Maximum number of non-diurnal emis types
        INTEGER    MXM5DIU          ! Maximum number of diurnal emis types
        INTEGER    MXM5EFS          ! Maximum number of emis types for one run
        INTEGER    MXM5ALL          ! Maximum number of all emis types
        INTEGER    MXM5SCEN         ! Maximum number of scenarios in one M5 run
        INTEGER    MXM5VTYP         ! Max no of vehicle types in mobile 5

        PARAMETER( NM5VPOL  = 6
     &           , MXM5EPR  = 9
     &           , MXM5NDI  = 7
     &           , MXM5DIU  = 4
     &           , MXM5EFS  = MXM5NDI + MXM5DIU
     &           , MXM5ALL  = 2 + NM5VPOL*( MXM5NDI-2 ) + 
     &                            NM5VPOL*MXM5DIU 
     &           , MXM5SCEN = 25
     &           , MXM5VTYP = 9
     &           )

C.........  Hydrocarbon type (all possible types) - 1st 5 in MOBILE5 order
        CHARACTER*3 M5VPOLS( NM5VPOL )
        DATA M5VPOLS / 'THC', 'NMH', 'VOC', 'TOG', 'NMO', 'ROG' /

C.........  Emissions processes (all possible types)
        CHARACTER*3 MVPROCS( MXM5EPR )
        DATA MVPROCS / 'EXH', 'EVP', 'RNL', 'RST', 'RFL',
     &                 'WDL', 'DNL', 'HOT', 'CRC'         /

C.........  Mobile5 vehicle types
        CHARACTER(LEN=VTPLEN3) M5VTYPES( MXM5VTYP )
        DATA M5VTYPES / 'LDGV', 'LDGT1', 'LDGT2', 'HDGV', 'LDDV',
     &                  'LDDT', 'HDDV' , 'MC'   , 'LDGT' /

